{
  "name": "rtlamr2mqtt",
  "version": "dev",
  "slug": "rtlamr2mqtt",
  "panel_icon": "mdi:transmission-tower",
  "description": "rtlamr2mqtt Add-ons",
  "url": "https://github.com/irakhlin/addon-rtlamr2mqtt",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "ingress": false,
  "startup": "application",
  "host_network": false,
  "usb": true,
  "tmpfs": true,
  "full_access": true,
  "boot": "auto",
  "environment": {
    "CONFIG_FILE": "/etc/rtlamr2mqtt.yaml"
  },
  "options": {
    "mqtt_host": "127.0.0.1",
    "mqtt_user": "user",
    "mqtt_password": null,
    "meter_id": 1550337017
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "meter_id": "int"
  },
  "services": ["mqtt:need"],
  "map": [
    "config:rw"
  ]
}
